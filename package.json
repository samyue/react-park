{
  "name": "react-park",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "eslint": "eslint \"src/**/*.{ts,tsx}\"",
    "stylelint": "stylelint src/**/*.scss --syntax scss"
  },
  "dependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.11",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@types/reactstrap": "8.0.4",
    "bootstrap": "4.3.1",
    "node-sass": "4.12.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0",
    "reactstrap": "8.0.1",
    "typescript": "3.6.3"
  },
  "devDependencies": {
    "eslint-config-airbnb": "18.0.1",
    "husky": "3.0.8",
    "lint-staged": "9.4.1",
    "prettier": "1.18.2",
    "prettier-stylelint": "0.4.2",
    "stylelint": "11.0.0",
    "stylelint-scss": "3.11.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.scss": [
      "stylelint --syntax scss --fix",
      "git add"
    ],
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
